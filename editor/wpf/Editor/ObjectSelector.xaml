<Window x:Class="Editor.ObjectSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Type Selector" Height="400" Width="300"
        ResizeMode="CanResizeWithGrip"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>            
        </Grid.RowDefinitions>
        
      <ListBox x:Name="m_list" Background="{DynamicResource DarkBackgroundBrush}" /> 
        <Button Grid.Row="1" Content="OK" Click="Button_Click_1"></Button>
    </Grid>
</Window>
