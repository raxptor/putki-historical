<Window x:Class="Editor.NewObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New Object" Height="100" Width="400"
        ResizeMode="CanResizeWithGrip"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="25"></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>            
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" x:Name="m_path" VerticalContentAlignment="Center"/>
        <ComboBox Grid.Row="1" x:Name="m_types" Height="25"/>
        <Button Grid.Row="2" Content="OK" Click="Button_Click_1"></Button>
    </Grid>
</Window>
