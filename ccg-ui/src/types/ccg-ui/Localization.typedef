#include Font.typedef

enum Language
{
	English
	Swedish
	French
	German
	Chinese
}

Translation [in, out]
{
	enum Language Lang = English
	string SID
	string Value
}

StringDatabase [in, out]
{
	string[] SIDs
	Translation[] Translations
}

LocalizationFont [in, out]
{
	file Source
	ptr StringDatabase StringsDB
	enum Language Lang
	[hidden] Font RealFont
}
