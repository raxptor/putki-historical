#include Texture.typedef

AtlasEntry [in, out]
{
	string id
	float u0
	float v0
	float u1
	float v1
}

AtlasOutput [in, out]
{
	int Width
	int Height
	float Scale
	AtlasEntry[] Entries
	ptr Texture Texture
}

Atlas [in, out]
{
	ptr[] Texture Inputs
	[no-out] int ScaleDowns
	[hidden] AtlasOutput[] Outputs
}
