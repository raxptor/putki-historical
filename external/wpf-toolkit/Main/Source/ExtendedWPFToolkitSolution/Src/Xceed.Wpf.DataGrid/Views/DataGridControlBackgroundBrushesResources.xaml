   <!-- sunriseBrush -->
   <VisualBrush x:Key="sunriseBrush">
      <VisualBrush.Visual>
         <!-- This grid will have a "natural" size because of the path. We don't have
              to set an arbitrary size. -->
         <Grid>
            <Grid.RowDefinitions>
               <RowDefinition Height="0.5*"/>
               <RowDefinition Height="0.5*"/>
            </Grid.RowDefinitions>

            <Border Background="#FFFFAB00"/>

            <Border>
               <Border.Background>
                  <LinearGradientBrush EndPoint="0.5,0"
                                       StartPoint="0.5,1">
                     <GradientStop Color="#7F670700"
                                   Offset="1"/>

                     <GradientStop Color="#00FF8800"
                                   Offset="0"/>
                  </LinearGradientBrush>
               </Border.Background>
            </Border>

